            <img src="{{asset('logos/'~trust.link)}}" class="logo-partenaire"/>
        {% endfor %}
        </div>
        <div class=" col-sm-6  col-md-6 bg-white">
        <h2>Engagés avec nos partenaires métiers:</h2>
        {%for partner in partners %}
           <img src="{{asset('logos/'~partner.link)}}"  class="logo-partenaire"/>
        {% endfor %}
        </div>
    </div>
{% endblock %}

