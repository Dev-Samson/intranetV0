{% extends "WebSiteFrontEndBundle::base.html.twig" %}

{% block body %}
    <div class="row partenaire page2" >
        <div class=" col-sm-3 col-md-3 bg-white">
            &nbsp
        </div>
        <div class=" col-sm-8 col-md-8 bg-white">
            <h1>Partenaires & Associations</h1>
        </div>
        <div class=" col-sm-1 col-md-1 bg-white">
            &nbsp
        </div>
    </div>
    <div class="row partenaire page2" >
        <div class=" col-sm-6 col-md-6 bg-white problemMargin">
        <h2>Il nous font confiances:</h2>
